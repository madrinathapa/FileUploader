#dvHeader {
    position: fixed;
    width: 100%;
    height: 30px;
    background-color: #5C9CCE;
    color: white;
    text-align: right;
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
    font-size: 0.9em;
    font-weight: bold;
    margin-top:-8px;
    margin-left:-8px;
    padding-right: 15px;
}
.ui-widget-header{
    background:#72A9D3;
    border: 1px solid #72A9D3;
    color: white;
    font-weight: bold;
}
.nav > li > a:hover, .nav > li > a:focus {
    background-color : #CCFFFF;
    color: #72A9D3;
}
.success:hover, .success:focus {
    background-color :#5C9CCE !important;
    color: white !important;
    text-decoration:none !important;
}

.nav > li > a {
    color: white;
}
.navbar-header a {
    color:white;
}

.navbar-inverse {
    background-color: #72A9D3 !important;
}

.navbar-inverse .navbar-brand {
    color: white !important;
}

.navbar-inverse .navbar-nav > .active > a, .navbar-inverse .navbar-nav > .active > a:hover, .navbar-inverse .navbar-nav > .active > a:focus {
    background-color: #72a9d3 !important;
    color: #fff !important;
}

.navbar-inverse .navbar-nav > li > a:hover, .navbar-inverse .navbar-nav > li > a:hover
{
background-color: #72a9d3 !important;
color: #fff !important;
}

.navbar-inverse .navbar-nav > li > a {
    color: white  !important;
}
/*For Mobile version nav bar button*/
.navbar-inverse .navbar-toggle:hover , .navbar-inverse .navbar-toggle:focus{
    background-color: #44657F !important;
}

/*For right side nav bar menu*/
.navbar-inverse .navbar-nav > .open > a, .navbar-inverse .navbar-nav > .open > a:hover, .navbar-inverse .navbar-nav > .open > a:focus {
    background-color: #72a9d3 !important;
    color: #fff;
}

/*For right side nav bar mobile version dropdown menu*/
.navbar-inverse .btn-link {
    color: white  !important;
}
.dropdown-menu {
    background-color :#72a9d3 !important;
}


